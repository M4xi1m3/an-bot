    "discord-api-types": "^0.36.0",
    "discord.js": "^13.8.1",
    "dotenv": "^16.0.1",
    "fast-xml-parser": "^4.0.8",
    "ffmpeg-static": "^5.0.2",
    "fluent-ffmpeg": "^2.1.2",
    "html-entities": "^2.3.3",
